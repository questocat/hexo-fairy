<!-- pagination -->
<ul class="pagination">
    <% if (page.prev) { %>
        <li class="previous"><a href="<%- url_for(page.prev_link) %>">Previous</a></li>
    <% } else { %>
        <li class="previous disabled"><span>Previous</span></li>
    <% } %>
    <% if (page.next) { %>
        <li class="next"><a href="<%- url_for(page.next_link) %>">Next</a></li>
    <% } else { %>
        <li class="next disabled"><span>Next</span></li>
    <% } %>
</ul>
<!-- End pagination -->
