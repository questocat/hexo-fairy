---
layout: layout
---
<section class="post">
    <header>
        <a href="/" class="backLink">&laquo; Back to article overview.</a>
        <h1><%- page.title %></h1>
        <!-- time -->
        <time datetime="<%- date(page.date, 'YYYY-MM-DD') %>" pubdate>
            <%- date(page.date, 'DD. MMM YYYY') %>
        </time>
    </header>

    <section>
        <!-- content -->
        <%- page.content %>
    </section>

    <%- partial('_partial/footer') %>
</section>
